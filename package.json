{
	"name": "auto-diary",
	"displayName": "auto-diary",
	"publisher": "ruanxihao",
	"description": "",
	"icon": "img/logo.png",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.30.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Xixi20160512/auto-diary"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/.auto-diary.json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.syncRemote",
				"title": "auto diary: sync"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"@types/shelljs": "^0.8.1",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"dayjs": "^1.8.13",
		"shelljs": "^0.8.5"
	}
}
